<div class="container">

  <div class="title"><h1>Track legislators by state</h1></div>

  <div>
    <%= form_tag("/states/new", method: :get) do %>
    <%= submit_tag 'Add' %>
    <% end %>
  </div>

  <div>
    <% @states.each do |state| %>
      <div><a href="/states/<%= state.id %>"><%= state.name %></a></div>
    <% end %>
  </div>
</div>
